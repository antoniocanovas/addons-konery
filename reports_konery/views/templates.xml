<?xml version='1.0' encoding='UTF-8'?>

<odoo>
    <data>

    <template id="reports_konery.report_invoice_custom" inherit_id="account.report_invoice" active="True">
          <xpath expr="//t[@t-lang='lang']" position="replace">
               <t t-if="o.move_report_type == 'konery'" t-call="account.report_invoice_document_kn" t-lang="lang"/>
               <t t-if="o.move_report_type in ['solarteam']" t-call="account.report_invoice_document_st" t-lang="lang"/>
               <t t-else="" t-call="account.report_invoice_document" t-lang="lang"/>
            </xpath>
    </template>

    </data>
</odoo>